x <- c(2, 4, 5, 6, 8, 11)
y <- c(18, 12, 10, 8, 7, 5)
model <- lm(y ~ x)
summary(model)
new_data <- data.frame(x = 7)
predicted_y <- predict(model, newdata = new_data)
cat("Regression Equation =",round(coefficients(model)[1], 2),"+",round(coefficients(model)[2], 2),"x\n")
cat("Predicted y when x=7:", round(predicted_y, 2), "\n")
